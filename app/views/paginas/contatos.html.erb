<h1>Contatos</h1>

<table class="table">
  <thead>
    <tr>
      <th>Id</th>
      <th>Nome</th>
      <th>Email</th>
      <th>Mensagem</th>
      <th>Data</th>
    </tr>
  </thead>

  <tbody>
    <% for contato in @contatos %>
    <tr>
      <td><%= contato.id %></td>
      <td><%= contato.nome %></td>
      <td><%= contato.email %></td>
      <td><%= contato.mensagem %></td>
      <td><%= l contato.created_at %></td>
    </tr>
    <% end %>
  </tbody>
</table>